{
  "name": "tenderbot",
  "version": "1.0.0",
  "description": "AI-powered tender document analyzer",
  "private": true,
  "scripts": {
    "dev": "concurrently \"cd server && npm run dev\" \"cd client && npm start\"",
    "build": "cd client && npm run build",
    "start": "cd server && npm start",
    "install-all": "npm install && cd client && npm install && cd ../server && npm install"
  },
  "devDependencies": {
    "concurrently": "^8.2.2"
  },
  "engines": {
    "node": ">=16.0.0"
  },
  "repository": {
    "type": "git",
    "url": "https://github.com/SaiNikheel/tenderBot.git"
  },
  "keywords": [
    "tender",
    "ai",
    "document-analysis",
    "procurement",
    "compliance"
  ],
  "author": "SaiNikheel",
  "license": "MIT"
} 